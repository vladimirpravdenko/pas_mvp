{
  "functions": {
    "api/suno-webhook.ts": {
      "runtime": "nodejs18.x"
    },
    "api/webhook-data.ts": {
      "runtime": "nodejs18.x"
    },
    "api/proxy-audio.ts": {
      "runtime": "nodejs18.x"
    }
  },
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "/(.*)", "dest": "/index.html" }
  ],
  "env": {
    "SUPABASE_URL": "@supabase-url",
    "SUPABASE_KEY": "@supabase-key"
  }
}
